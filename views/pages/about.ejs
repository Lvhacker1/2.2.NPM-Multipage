<%-include ("../partials/head", {pageTitle: "About"})%>
<%-include ("../partials/header", {page: "About"})%>

<section class="main_section_about">
    <div class="about_container">
        <h1 class="page_title_about"><%- about.intro.title %></h1>
        <p class="intro_text_about"><%- about.intro.text %></p>
    </div>
    <div class="about_links">
        <% about.sections.forEach(section => { %>
            <a class="sub_links" href="/about/<%= section.slug %>"><%= section.link %> </a>
        <% }) %>
    </div>
</section>

<%-include ("../partials/footer")%>